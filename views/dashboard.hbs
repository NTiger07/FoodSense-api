<h6>Dashboard</h6>
<h3>Welcome {{name}}</h3>

{{#if items}}
  <span>Here are your Items</span>

  <table>
    <thead>
      <tr>
        <th>#</th>
        <th>Name</th>
        <th>Units</th>
        <th>Expiry Date</th>
        <th>Notes</th>
      </tr>
    </thead>
    <tbody>
      {{#each items}}
        <tr>
          <td>{{itemId}}</td>
          <td>{{itemName}}</td>
          <td>{{units}}</td>
          <td>{{formatDate expiryDate 'Do MMMM YYYY, h:mm:ss a'}}</td>
          <td>{{itemNotes}}</td>
          <td>{{formatDate createdAt 'Do MMMM YYYY, h:mm:ss a'}}</td>
        </tr>
      {{/each}}
    </tbody>
  </table>

{{else}}
  <p>You don't have any items</p>
{{/if}}
